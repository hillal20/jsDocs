{
  "source": {
    "include": ["src"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "moonscapeLinks": true
  },
  "opts": {
    "rescue": true,
    "destination": "./docs",
    "template": "./custom-template",
    "tutorials": "./tutorials"
  }
}
